{
    "version": 2,
    "builds": [
        {
            "src": "./app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],

    "functions": {
        "api/test.js": {
          "memory": 3008,
          "maxDuration": 30
        },
        "api/*.js": {
          "memory": 3008,
          "maxDuration": 30
        }
      }
}